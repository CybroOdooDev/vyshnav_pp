<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="stock.mail_template_data_delivery_confirmation" model="mail.template">
            <field name="name">Delivery: Send by Email1</field>
            <field name="subject">Your order is complete</field>
            <field name="email_from">{{ (object.partner_id.email or '') }}</field>
            <field name="partner_to">{{object.partner_id.id}}</field>
            <field name="model_id" ref="delivery.model_stock_picking"/>
            <field name="report_template" eval="False"/>
            <field name="report_name" eval="False"/>
            <field name="lang">{{object.partner_id.lang}}</field>
            <field name="body_html" type="html">
                <p style="box-sizing:border-box;margin-bottom: 0px;">
                    <style>
                        @import url('https://fonts.googleapis.com/css2?family=Raleway:wght@900&amp;display=swap');
                    </style>
                </p>
                <div style="padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777">
